{
  "expo": {
    "name": "instadaelim_typescript",
    "slug": "instadaelim_typescript",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "이미지 선택을 위한 권한을 설정하세요."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME)(이)가 앨범에 접근권한을 설정해주세요.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME)(이)가 사진을 가져오는 것을 설정해주세요.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
